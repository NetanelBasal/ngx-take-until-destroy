{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "ngx-take-until-destroy": [
        "./dist"
      ]
    },
    "target": "es5",
    "outDir": "dist/",
    "lib": [
      "dom",
      "es2017"
    ],
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
